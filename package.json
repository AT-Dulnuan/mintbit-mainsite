  "dependencies": {
    "lightgallery.js": "^1.1.3",
    "luminous-lightbox": "^2.3.2",
    "simple-lightbox": "^2.1.0"
  },
  "devDependencies": {
    "node-sass": "^4.12.0"
  }
}
